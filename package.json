{
  "name": "sri-lanka-police-stations",
  "version": "1.0.0",
  "description": "Police stations database for finding nearest stations in Sri Lanka",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "test": "node --test test/*.test.js",
    "test:distance": "node --test test/distance.test.js",
    "test:service": "node --test test/policeStationsService.test.js",
    "test:integration": "node --test test/integration.test.js",
    "test:watch": "node --test --watch test/*.test.js",
    "test:coverage": "npm test",
    "test:status": "node scripts/test-status.js",
    "test:ci": "npm test && npm run test:coverage",
    "validate": "node scripts/validate.js",
    "benchmark": "node scripts/benchmark.js"
  },
  "keywords": [
    "sri-lanka",
    "police-stations",
    "geolocation",
    "distance-calculation"
  ],
  "author": "Police Stations System",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}